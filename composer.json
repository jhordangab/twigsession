{
    "name": "jhordangab/twig-session",
    "description": "Twig session",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "twig session"
    ],
    "support": {
        "docs": "https://github.com/jhordangab/twig-session/blob/master/README.md",
        "issues": "https://github.com/jhordangab/twig-session/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "jhordangab",
            "homepage": "https://github.com/jhordangab/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "jhordangab\\twigsession\\": "src/"
        }
    },
    "extra": {
        "name": "Twig Session",
        "handle": "twig-session",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/jhordangab/twig-session/master/CHANGELOG.md",
        "class": "jhordangab\\twigsession\\TwigSession"
    }
